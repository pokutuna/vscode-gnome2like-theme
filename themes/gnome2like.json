			],
			"settings": {
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "CSS Values",
			"scope": [
				"support.constant.property-value.css",
				"support.constant.font-name.css",
				"support.constant.color.css"
			],
			"settings": {
				"foreground": "#FFA07A"
			}
		},
		{
			"name": "CSS Classes and IDs",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal",
				"invalid.deprecated"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Markdown Headings",
			"scope": [
				"markup.heading",
				"markup.heading entity.name",
				"markup.heading.markdown punctuation.definition.heading.markdown"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "Markdown Links",
			"scope": [
				"string.other.link.title.markdown",
				"string.other.link.description.title.markdown",
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#FA8072"
			}
		},
		{
			"name": "Markdown Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#F5DEB3"
			}
		},
		{
			"name": "Markdown Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#F5DEB3"
			}
		},
		{
			"name": "Markdown Code",
			"scope": [
				"markup.inline.raw",
				"markup.raw.block",
				"markup.fenced_code",
				"markup.fenced_code.block"
			],
			"settings": {
				"foreground": "#98FB98"
			}
		},
		{
			"name": "Diff Added",
			"scope": [
				"markup.inserted",
				"meta.diff.header.to-file"
			],
			"settings": {
				"foreground": "#98FB98"
			}
