		"enum": {
			"foreground": "#9ACD32",
			"bold": true
		},
		"struct": {
			"foreground": "#9ACD32",
			"bold": true
		},
		"typeParameter": {
			"foreground": "#9ACD32"
		},
		"type": {
			"foreground": "#9ACD32",
			"bold": true
		},
		"type.defaultLibrary": {
			"foreground": "#98FB98",
