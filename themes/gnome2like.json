			],
			"settings": {
				"foreground": "#F5DEB3"
			}
		},
		{
			"name": "Methods",
			"scope": [
				"entity.name.method",
				"meta.method-call",
				"meta.method"
			],
			"settings": {
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "Builtin Functions",
