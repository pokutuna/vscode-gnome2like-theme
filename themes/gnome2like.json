		"editorGroup.border": "#008B8B",
		"editorGroupHeader.tabsBackground": "#2F4F4F",
		"editorGutter.addedBackground": "#98FB9880",
		"editorGutter.background": "#2F4F4F",
		"editorGutter.deletedBackground": "#FA807280",
		"editorGutter.modifiedBackground": "#008B8B80",
		"editorHoverWidget.background": "#2F4F4FE8",
		"editorHoverWidget.border": "#7FFFD4",
		"editorHoverWidget.foreground": "#F5DEB3",
		"editorHoverWidget.statusBarBackground": "#2A4545",
		"editorInfo.border": "#00000000",
		"editorInfo.foreground": "#7FFFD440",
		"editorLineNumber.activeForeground": "#F5DEB3",
		"editorLineNumber.foreground": "#F5DEB380",
		"editorLink.activeForeground": "#7FFFD4",
		"editorOverviewRuler.background": "#2F4F4F",
		"editorOverviewRuler.border": "#008B8B",
		"editorRuler.foreground": "#F5DEB320",
		"editorSuggestWidget.background": "#2F4F4F",
		"editorSuggestWidget.border": "#008B8B",
		"editorSuggestWidget.foreground": "#F5DEB3",
		"editorSuggestWidget.highlightForeground": "#7FFFD4",
		"editorSuggestWidget.selectedBackground": "#008B8B60",
		"editorWarning.border": "#00000000",
		"editorWarning.foreground": "#FFA07A",
		"editorWhitespace.foreground": "#F5DEB340",
		"editorWidget.background": "#2F4F4F",
		"editorWidget.border": "#008B8B",
		"editorWidget.foreground": "#F5DEB3",
		"editorWidget.resizeBorder": "#7FFFD4",
		"focusBorder": "#008B8B",
		"foreground": "#F5DEB3",
		"input.background": "#2F4F4F",
		"input.border": "#008B8B",
		"input.foreground": "#F5DEB3",
		"input.placeholderForeground": "#F5DEB380",
		"inputOption.activeBackground": "#008B8B40",
		"inputOption.activeBorder": "#008B8B",
		"inputOption.activeForeground": "#F5DEB3",
		"list.activeSelectionBackground": "#008B8B",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.dropBackground": "#008B8B40",
		"list.focusBackground": "#008B8B60",
		"list.focusForeground": "#F5DEB3",
		"list.highlightForeground": "#7FFFD4",
		"list.hoverBackground": "#008B8B20",
		"list.hoverForeground": "#F5DEB3",
		"list.inactiveSelectionBackground": "#008B8B40",
		"list.inactiveSelectionForeground": "#F5DEB3",
		"listFilterWidget.background": "#2F4F4F",
		"listFilterWidget.noMatchesOutline": "#FA8072",
		"listFilterWidget.outline": "#008B8B",
		"menu.background": "#2F4F4F",
		"menu.border": "#008B8B",
		"menu.foreground": "#F5DEB3",
		"menu.selectionBackground": "#008B8B",
		"menu.selectionForeground": "#FFFFFF",
		"menu.separatorBackground": "#F5DEB333",
		"menubar.selectionBackground": "#008B8B40",
		"menubar.selectionForeground": "#F5DEB3",
		"minimap.errorHighlight": "#FA807280",
		"minimap.findMatchHighlight": "#7FFFD480",
		"minimap.selectionHighlight": "#008B8B80",
		"minimap.warningHighlight": "#FFA07A80",
		"peekView.border": "#7FFFD4",
		"peekViewEditor.background": "#2A4545",
		"peekViewEditor.matchHighlightBackground": "#7FFFD440",
		"peekViewResult.background": "#2F4F4F",
		"peekViewResult.fileForeground": "#F5DEB3",
		"peekViewResult.lineForeground": "#ADD8E6",
		"peekViewResult.matchHighlightBackground": "#7FFFD440",
		"peekViewResult.selectionBackground": "#008B8B60",
		"peekViewResult.selectionForeground": "#FFFFFF",
		"peekViewTitle.background": "#2A4545",
		"peekViewTitleDescription.foreground": "#ADD8E680",
		"peekViewTitleLabel.foreground": "#F5DEB3",
		"pickerGroup.border": "#F5DEB333",
		"pickerGroup.foreground": "#F5DEB3",
		"scrollbar.shadow": "#00000070",
		"selection.background": "#008B8B",
		"sideBar.background": "#2F4F4F90",
		"sideBar.border": "#008B8B",
		"sideBar.foreground": "#F5DEB3",
		"sideBarSectionHeader.background": "#2F4F4F",
		"sideBarSectionHeader.border": "#F5DEB333",
		"sideBarSectionHeader.foreground": "#F5DEB3",
		"sideBarTitle.foreground": "#F5DEB3",
		"statusBar.background": "#556B2F",
		"statusBar.debuggingBackground": "#008B8B",
		"statusBar.foreground": "#F5DEB3",
		"statusBar.noFolderBackground": "#2F4F4F80",
		"statusBar.noFolderForeground": "#F5DEB380",
		"statusBarItem.remoteBackground": "#556B2F",
		"statusBarItem.remoteForeground": "#F5DEB3",
		"statusBarItem.warningBackground": "#3E4E22",
		"statusBarItem.warningForeground": "#FFFFFF",
		"tab.activeBackground": "#2F4F4F",
		"tab.activeBorderTop": "#008B8B",
		"tab.activeForeground": "#F5DEB3",
		"tab.inactiveBackground": "#2A4545",
		"tab.inactiveForeground": "#F5DEB380",
		"tab.lastPinnedBorder": "#F5DEB333",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#7FFFD4",
		"terminal.ansiGreen": "#98FB98",
		"terminal.ansiRed": "#FA8072",
		"terminal.ansiWhite": "#F5DEB3",
		"terminal.ansiYellow": "#FFA07A",
		"terminal.background": "#2F4F4F",
		"terminal.foreground": "#F5DEB3",
		"terminal.selectionBackground": "#008B8B60",
		"textLink.foreground": "#7FFFD4",
		"titleBar.activeBackground": "#2F4F4F",
		"titleBar.activeForeground": "#F5DEB3",
		"titleBar.inactiveBackground": "#2F4F4F80",
		"titleBar.inactiveForeground": "#F5DEB380",
		"tree.indentGuidesStroke": "#F5DEB340",
		"widget.shadow": "#00000070",
		"badge.background": "#008B8B",
		"badge.foreground": "#FFFFFF",
		"panelTitle.activeForeground": "#F5DEB3",
		"panelTitle.inactiveForeground": "#F5DEB380",
		"icon.foreground": "#F5DEB3"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#ADD8E6"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.quoted",
				"string.quoted.single",
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#FFA07A"
			}
		},
		{
			"name": "String Doc",
			"scope": [
				"string.quoted.docstring",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#FFA07A"
			}
		},
		{
			"name": "Number & Constant",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator",
				"storage",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#FA8072"
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"entity.name.function.preprocessor",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#FA8072"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"variable.parameter",
				"variable.language",
				"variable.name"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "Function & Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"meta.function",
				"support.function",
				"variable.function"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#98FB98"
			}
		},
		{
			"name": "Type",
			"scope": [
				"entity.name.type",
				"support.type",
				"storage.type.cs",
				"entity.name.class",
				"entity.other.inherited-class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#9ACD32"
			}
		},
		{
			"name": "Built-in",
			"scope": [
				"support.function",
				"support.class",
				"support.type",
				"support.constant",
				"support.variable"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#98FB98"
			}
		},
		{
			"name": "Reference",
			"scope": [
				"meta.preprocessor",
				"entity.name.function",
				"meta.require",
				"support.function.any-method"
			],
			"settings": {
				"foreground": "#98FB98"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal",
				"invalid.deprecated"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "JSON Key",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "JSON Value",
			"scope": [
				"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json"
			],
			"settings": {
				"foreground": "#F5DEB3"
			}
		},
		{
			"name": "Markdown Heading",
			"scope": [
				"markdown.heading",
				"markup.heading | markup.heading entity.name",
				"markup.heading.markdown punctuation.definition.heading.markdown"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "Markdown Link",
			"scope": [
				"string.other.link.title.markdown",
				"string.other.link.description.title.markdown"
			],
			"settings": {
				"foreground": "#FA8072"
			}
		},
		{
			"name": "Markup Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#F5DEB3"
			}
		},
		{
			"name": "Markup Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#F5DEB3"
			}
		},
		{
			"name": "Markup Code",
			"scope": [
				"markup.inline.raw",
				"markup.raw.block"
			],
			"settings": {
				"foreground": "#98FB98"
			}
		},
		{
			"name": "CSS Properties",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.scss",
				"support.type.property-name.less",
				"support.type.property-name.sass"
			],
			"settings": {
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "CSS Values",
			"scope": [
				"support.constant.property-value.css",
				"support.constant.font-name.css",
				"support.constant.color.css"
			],
			"settings": {
				"foreground": "#FFA07A"
			}
		},
		{
			"name": "HTML Tag",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#FA8072"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "Diff Added",
			"scope": [
				"markup.inserted",
				"meta.diff.header.to-file"
			],
			"settings": {
				"foreground": "#98FB98"
			}
		},
		{
			"name": "Diff Removed",
			"scope": [
				"markup.deleted",
				"meta.diff.header.from-file"
			],
			"settings": {
				"foreground": "#FA8072"
			}
		},
		{
			"name": "Diff Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#FFA07A"
			}
		},
		{
			"name": "Diff Header",
			"scope": [
				"meta.diff.header",
				"meta.diff.range"
			],
			"settings": {
				"foreground": "#ADD8E6"
			}
		}
	],
	"semanticTokenColors": {
		"property": "#7FFFD4",
		"type": {
			"foreground": "#9ACD32",
			"bold": true
		},
		"class": {
			"foreground": "#9ACD32",
			"bold": true
		},
		"enum": {
			"foreground": "#9ACD32",
			"bold": true
		},
		"interface": {
			"foreground": "#9ACD32",
			"bold": true,
			"italic": true
		},
		"function": {
			"foreground": "#7FFFD4",
			"bold": true
		},
		"method": {
			"foreground": "#7FFFD4",
			"bold": true
		},
		"macro": {
			"foreground": "#FA8072"
		},
		"comment": {
			"foreground": "#ADD8E6",
			"italic": true
		},
		"string": "#FFA07A",
		"keyword": "#FA8072",
		"number": "#7FFFD4",
		"regexp": "#FFA07A",
		"operator": "#FA8072"
	},
}
