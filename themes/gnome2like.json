				"entity.name.type",
				"entity.name.class",
				"entity.other.inherited-class",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#9ACD32",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Namespaces & Modules",
