		"editor.inactiveSelectionBackground": "#008B8B30",
		"editor.lineHighlightBackground": "#008B8B20",
		"editor.lineHighlightBorder": "#008B8B20",
		"editor.rangeHighlightBackground": "#008B8B20",
		"editor.rangeHighlightBorder": "#008B8B40",
		"editor.selectionBackground": "#008B8B80",
		"editor.selectionHighlightBackground": "#008B8B40",
		"editor.selectionHighlightBorder": "#008B8B",
		"editor.wordHighlightBackground": "#008B8B30",
		"editor.wordHighlightStrongBackground": "#008B8B50",
		"editorBracketMatch.background": "#008B8B40",
		"editorBracketMatch.border": "#008B8B",
		"editorCursor.background": "#2F4F4F",
		"editorCursor.foreground": "#F5DEB3",
		"editorError.border": "#00000000",
