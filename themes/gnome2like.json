			],
			"settings": {
				"foreground": "#FA8072"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#7FFFD4"
			}
		},
		{
			"name": "Properties",
